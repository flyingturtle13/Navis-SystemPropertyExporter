<RibbonControl
    x:Uid="SystemPropertyExporter" xmlns="clr-namespace:Autodesk.Windows;assembly=AdWindows"
    xmlns:wpf="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:adwi="clr-namespace:Autodesk.Internal.Windows;assembly=AdWindows"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Autodesk.Navisworks.Gui.Roamer.AIRLook;assembly=navisworks.gui.roamer">
  <!-- END STANDARD SECTION DO NOT EDIT -->

  <RibbonTab Id="VDC Add-Ins" KeyTip="T1">
    <RibbonPanel x:Uid="RibbonPane1_1">
      <RibbonPanelSource x:Uid="RibbonPanelSource_RibbonPanel_1" KeyTip="C1" Title="Export Tools">
        <RibbonRowPanel x:Uid="RibbonRowPanel_1">
            <local:NWRibbonButton x:Uid="Clash Data Exporter" Id="Clash_Data_Exporter"
                    Size="Large"
                    KeyTip="B1"
                    ShowText="True"
                    Text="Clash Data Exporter"
                    Orientation="Vertical"/>
          
            <local:NWRibbonButton x:Uid="System Property Exporter" Id="System_Property_Exporter"
                    Size="Large"
                    KeyTip="B2"
                    ShowText="True"
                    Text="System Property Exporter"
                    Orientation="Vertical"/>
        </RibbonRowPanel>
      </RibbonPanelSource>
    </RibbonPanel>
  </RibbonTab>

</RibbonControl>